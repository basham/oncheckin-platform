{
	"name": "@oncheckin/assets",
	"version": "1.0.0",
	"private": true,
	"description": "Generate shared assets for other packages.",
	"type": "module",
	"scripts": {
		"dev": "pnpm --filter assets build",
		"build": "run-s build:copy build:images build:meta",
		"build:copy": "cpx src/logo.svg dist",
		"build:images": "pwa-assets-generator",
		"build:meta": "node pwa-assets.js"
	},
	"devDependencies": {
		"@vite-pwa/assets-generator": "1.0.2",
		"cpx": "1.5.0",
		"npm-run-all": "4.1.5"
	}
}
